{
	"name": "haxibiao/graphql",
	"description": "Facebook GraphQL for Laravel",
	"keywords": [
		"framework",
		"laravel",
		"graphql",
		"react"
	],
	"authors": [
		{
			"name": "Folklore",
			"email": "info@atelierfolklore.ca",
			"homepage": "http://atelierfolklore.ca"
		},
		{
			"name": "David Mongeau-Petitpas",
			"email": "dmp@atelierfolklore.ca",
			"homepage": "http://mongo.ca",
			"role": "Developer"
		}
	],
	"license": "MIT",
	"type": "library",
	"require": {
		"php": ">=5.5.9",
		"illuminate/support": "5.8.30",
		"webonyx/graphql-php": "0.10.2"
	},
	"require-dev": {
		"orchestra/testbench": "3.5.*",
		"fzaninotto/faker": "1.4",
		"mockery/mockery": "0.9.1",
		"satooshi/php-coveralls": "1.0",
		"phpunit/phpunit": "6.0"
	},
	"autoload": {
		"psr-0": {
			"Folklore\\GraphQL\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"suggest": {
		"rebing/graphql-laravel": "folklore/graphql is no longuer maintained. Please use rebing/graphql-laravel"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Folklore\\GraphQL\\ServiceProvider"
			],
			"aliases": {
				"GraphQL": "Folklore\\GraphQL\\Support\\Facades\\GraphQL"
			}
		}
	}
}
